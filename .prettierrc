{
  "semi": true,
  "singleQuote": true,
  "jsxSingleQuote": false,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "arrowParens": "avoid",
  "printWidth": 80,
  "endOfLine": "auto",
  "overrides": [
    {
      "files": "*.jsx",
      "options": {
        "jsxBracketSameLine": true
      }
    }
  ]
}